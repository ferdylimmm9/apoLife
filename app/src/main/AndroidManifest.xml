<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apolifee">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ApoLifee">
        <activity
            android:name=".activity.listObat"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            />
        <activity
            android:name=".activity.listBerita"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            />
        <activity
            android:name=".activity.bacaObat"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            />
        <activity
            android:name=".activity.bacaBerita"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activity.login"
            android:exported="false" />
        <activity
            android:name=".activity.register"
            android:exported="false"
            android:parentActivityName=".activity.login" />
        <activity
            android:name=".splashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false"></activity>
    </application>

</manifest>